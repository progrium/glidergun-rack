# install: mv google-cloud-sdk $PREFIX && ./google-cloud-sdk/install.sh --usage-reporting=false --path-update=false --bash-completion=false && cp $PREFIX/google-cloud-sdk/bin/gcloud $PREFIX/bin/ && sed -i "/^CLOUDSDK_ROOT_DIR/ s/^/# /"  $PREFIX/bin/gcloud
latest * https://dl.google.com/dl/cloudsdk/release/google-cloud-sdk.zip
